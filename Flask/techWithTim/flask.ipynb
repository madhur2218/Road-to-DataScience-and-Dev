    "from flask import Flask, render_template\n",
    "\n",
    "app = Flask(__name__)\n",
    "\n",
    "@app.route(\"/<name>\")\n",
    "def user(name):\n",
    "    return render_template(\"index.html\", content=name, r=21, content1=['tim', 'madhur', 'ashu', 'pj'])\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    app.run()\n"
